x = int(input())
def prime(x):
    if x <= 1:
        return False
    for i in range(2,int(x**0.5)+1):
        if x%i==0:
            return False
    return True
def greather(x):
    if prime(x):
        while prime(x + 1) is False:
            x+=1
        return x+1
    while prime(x) is False:
        x+=1
    return x
print(greather(x))
